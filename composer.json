{
  "name": "exbico/monolog-db-handler",
  "description": "DB handler for monolog",
  "keywords": [
    "monolog",
    "handler",
    "database"
  ],
  "type": "library",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Contributors",
      "homepage": "https://github.com/exbico/monolog-db-handler/contributors"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-pdo": "*",
    "monolog/monolog": "^2.3"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Exbico\\Handler\\": "src/"
    }
  }
}
